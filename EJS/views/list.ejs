<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Fruit List</h1>
    <ul>
        <% if(arr.length==0){ %>
            <li>No fruits available</li>
        <% } else { %>
            <% arr.forEach(function(fruit){ %>
                <li><%= fruit %></li>
            <% }); %>
        <% } %>
    </ul>
</body>
</html>